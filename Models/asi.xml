<?xml version="1.0"?>

<PropertyList>

 <name>ASI</name>
 <path>asi.ac</path>

 <animation>
  <type>rotate</type>
  <object-name>needle</object-name>
  <property>instrumentation/airspeed-indicator/indicated-speed-kt</property>
  <interpolation>
   <entry><ind>  0</ind><dep>   0</dep></entry>
   <entry><ind>  10</ind><dep>   1.5</dep></entry>
   <entry><ind>  20</ind><dep>   4</dep></entry>
   <entry><ind>  40</ind><dep>   10</dep></entry>
   <entry><ind>  80</ind><dep>   21</dep></entry>
   <entry><ind>  100</ind><dep>   44</dep></entry>
   <entry><ind>  120</ind><dep>   73</dep></entry>
   <entry><ind>  150</ind><dep>   112</dep></entry>
   <entry><ind>  200</ind><dep>   148</dep></entry>
   <entry><ind>  250</ind><dep>  182</dep></entry>
   <entry><ind> 300</ind><dep>  212</dep></entry>
   <entry><ind> 350</ind><dep>  240</dep></entry>
   <entry><ind> 400</ind><dep>  266</dep></entry>
   <entry><ind> 450</ind><dep>  290</dep></entry>
   <entry><ind> 500</ind><dep>  310</dep></entry>
   <entry><ind> 600</ind><dep>  335</dep></entry>
  </interpolation>
  <axis>
   <x>-1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>


  <animation> 
   <object-name>mount</object-name>
   <type>material</type>
    <emission> 
      <factor-prop>controls/lighting/panel-norm</factor-prop>
        <red>0.45</red>
        <green>0.34</green>
        <blue>0.18</blue>
      </emission>
  </animation>

  <animation>
   <object-name>face</object-name>
   <type>material</type>
     <emission> 
      <factor-prop>controls/lighting/instruments-norm</factor-prop>
        <red>0.50</red>
        <green>0.25</green>
        <blue>0.18</blue>
      </emission>
  </animation>


  <animation>
   <object-name>needle-w</object-name>
   <type>material</type>
    <emission> 
      <factor-prop>controls/lighting/instruments-norm</factor-prop>
        <red>0.75</red>
        <green>0.55</green>
        <blue>0.35</blue>
      </emission>
  </animation>


 <animation>
  <type>range</type>
  <min-m>0</min-m>
  <max-m>5</max-m>
 </animation>

</PropertyList>
