<?xml version="1.0"?>
<PropertyList>

 <path>hook-lift.ac</path>

	<animation>
		<type>select</type>
		<object-name>hook-lift-bt-lighted</object-name>
		<condition>
			<less-than>
				<property>gear/tailhook/position-norm</property>
				<value>1</value>
			</less-than>
			<greater-than>
				<property>gear/tailhook/position-norm</property>
				<value>0.01</value>
			</greater-than>
		</condition>
	</animation>

	<animation>
		<type>select</type>
		<object-name>hook-lift-bt</object-name>
		<condition>
			<or>
				<less-than>
					<property>gear/tailhook/position-norm</property>
					<value>0.01</value>
				</less-than>
				<equals>
					<property>gear/tailhook/position-norm</property>
					<value>1</value>
				</equals>
			</or>
		</condition>
	</animation>

 
<!-- lighting -->

  <animation> 
   <object-name>mount</object-name>
   <type>material</type>
    <emission> 
      <factor-prop>controls/lighting/panel-norm</factor-prop>
        <red>0.45</red>
        <green>0.34</green>
        <blue>0.18</blue>
      </emission>
  </animation>

  <animation> 
   <object-name>fast-erect-bt</object-name>
   <type>material</type>
    <emission> 
      <factor-prop>controls/lighting/panel-norm</factor-prop>
        <red>0.45</red>
        <green>0.34</green>
        <blue>0.18</blue>
      </emission>
  </animation>

  <animation> 
   <object-name>hook-lift-bt</object-name>
   <type>material</type>
    <emission> 
      <factor-prop>controls/lighting/panel-norm</factor-prop>
        <red>0.45</red>
        <green>0.34</green>
        <blue>0.18</blue>
      </emission>
  </animation>

  <animation> 
   <object-name>hook-lift-bt-lighted</object-name>
   <type>material</type>
    <emission> 
        <red>1</red>
        <green>1</green>
        <blue>1</blue>
      </emission>
  </animation>

	<animation>
		<object-name>hooklift</object-name>
		<type>noshadow</type>
	</animation>


  <animation>
    <type>range</type>
    <min-m>0</min-m>
    <max-m>35</max-m>
  </animation>

</PropertyList>
